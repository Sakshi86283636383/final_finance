    <script>
    $(document).ready(function() {
      $('#contact-form').on('submit', function(event) {
        event.preventDefault(); // Prevent default form submission

        $.ajax({
          type: 'POST',
          url: '/sub',
          data: $(this).serialize(),
          beforeSend: function() {
            $('#loader').show();
          },
          success: function(response) {
            $('#loader').hide();
            alert('Your query is received. We will respond you soon.');
            $('#contact-form')[0].reset(); // Reset the form
          },
          error: function(error) {
            $('#loader').hide();
            alert('There was an error submitting your query: ' + error.responseJSON.errorMessage);
          }
        });
      });
    });
  </script>

